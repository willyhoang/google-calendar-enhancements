{
  "name": "Enhanced Google Calendar",
  "description": "Adds enhancements to Google Calendar.",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "https://maps.google.com/*",
    "https://maps.googleapis.com/*"
  ],
  "content_scripts": [{
    "matches": ["https://calendar.google.com/*"],
    "js": ["script.js"]
  }],
  "manifest_version": 2,
  "web_accessible_resources": ["content_script.js"]
}
